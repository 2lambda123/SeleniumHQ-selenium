module Selenium
  module WebDriver
    module Support
      class RelativeLocator
        @filters: untyped

        @root: untyped

        KEYS: ::Array["above" | "below" | "left" | "right" | "near" | "distance"]

        def initialize: (untyped locator) -> void

        def as_json: () -> { relative: { root: untyped, filters: untyped } }
      end
    end
  end
end
