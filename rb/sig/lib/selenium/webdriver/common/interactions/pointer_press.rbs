module Selenium
  module WebDriver
    module Interactions
      class PointerPress < Interaction
        @direction: untyped

        @button: untyped

        @type: untyped

        @opts: untyped

        include PointerEventProperties

        BUTTONS: untyped

        DIRECTIONS: { down: :pointerDown, up: :pointerUp }

        def initialize: (untyped source, untyped direction, untyped button, **untyped opts) -> void

        def encode: () -> untyped

        private

        def assert_source: (untyped source) -> (untyped | nil)

        def assert_button: (untyped button) -> untyped

        def assert_direction: (untyped direction) -> untyped
      end
    end
  end
end
